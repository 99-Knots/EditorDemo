{
  "compilerOptions": {
    "target": "es2016", 
    "lib": ["dom", "dom.iterable", "esnext"],                            
    "skipLibCheck": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": false,
    "strict": true,
    "jsx": "react",
    "outDir": "./dist"
  },
  "include": ["src", "assets"]
}
